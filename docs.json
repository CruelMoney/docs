  "logo": {
    "light": "/logo/dark.svg",
    "dark": "/logo/light.svg",
    "href": "https://moderationapi.com"
  },
  "redirects": [
    {
      "source": "/quickstart/nodejs",
      "destination": "/quickstart/typescript"
    },
    {
      "source": "/changelog",
      "destination": "https://product.moderationapi.com/changelog"
    },

    {
      "source": "/api-reference/moderate/analyze-text",
      "destination": "/api-reference/moderate/submit-content"
    },
    {
      "source": "/api-reference/moderate/analyze-object",
      "destination": "/api-reference/moderate/submit-content"
    },
    {
      "source": "/api-reference/moderate/analyze-image",
      "destination": "/api-reference/moderate/submit-content"
    },
    {
      "source": "/api-reference/moderate/analyze-video",
      "destination": "/api-reference/moderate/submit-content"
    },
    {
      "source": "/api-reference/moderate/analyze-audio",
      "destination": "/api-reference/moderate/submit-content"
    }
  ],
  "api": {
    "openapi": "https://app.stainless.com/api/spec/documented/moderation-api/openapi.documented.yml",
